{
  "name": "typescript-graphql-schemas",
  "private": true,
  "description": "Comparison of TypeScript GraphQL schema builders",
  "scripts": {
    "format": "prettier --write .",
    "dev:gqloom": "pnpm -F gqloom dev",
    "print:gqloom": "pnpm -F gqloom print",
    "test:gqloom": "pnpm -F gqloom test",
    "dev:pothos": "pnpm -F pothos dev",
    "print:pothos": "pnpm -F pothos print",
    "test:pothos": "pnpm -F pothos test",
    "dev:typegraphql": "pnpm -F typegraphql dev",
    "print:typegraphql": "pnpm -F typegraphql print",
    "test:typegraphql": "pnpm -F typegraphql test"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "prettier": "^3.4.2",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2"
  }
}
